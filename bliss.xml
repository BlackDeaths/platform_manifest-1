<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote name="BlissRoms"
          fetch="https://github.com/BlissRoms"
          review="review.blissroms.com" />

  <remote name="BlissRoms-Devices"
          fetch="https://github.com/BlissRoms-Devices" />

  <!-- Remove first some AOSP repos -->

  <remove-project name="platform/build" />
  <remove-project name="platform/build/kati" />
  <remove-project name="platform/bionic" />
  <remove-project name="platform/frameworks/av" />
  <remove-project name="platform/frameworks/base" />
  <remove-project name="platform/frameworks/native" />
  <remove-project name="platform/frameworks/support" />
  <remove-project name="platform/packages/apps/Settings" />
  <remove-project name="platform/system/core" />
  <remove-project name="platform/system/extras" />
  <remove-project name="platform/system/sepolicy" />
  <remove-project name="platfor/packages/providers/MediaProvider" />


  <!--                              -->

  <!-- Begin BlissRoms repos -->

  <project path="manifest" name="platform_manifest" remote="BlissRoms" revision="o8.0" />
  <project path="build/kati" name="platform_build_kati" remote="BlissRoms" revision="o8.0" />
  <project path="build/make" name="platform_build" remote="BlissRoms" revision="o8.0" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="bionic" name="platform_bionic" remote="BlissRoms" revision="o8.0" />
  <project path="frameworks/av" name="platform_frameworks_av" remote="BlissRoms" revision="o8.0" />
  <project path="frameworks/base" name="platform_frameworks_base" remote="BlissRoms" revision="o8.0" />
  <project path="frameworks/native" name="platform_frameworks_native" remote="BlissRoms" revision="o8.0" />
  <project path="frameworks/support" name="platform_frameworks_support" remote="BlissRoms" revision="o8.0" />
  <project path="packages/apps/Settings" name="platform_packages_apps_Settings" remote="BlissRoms" revision="o8.0" />
  <project path="system/core" name="platform_system_core" remote="BlissRoms" revision="o8.0" />
  <project path="system/extras" name="platform_system_extras" remote="BlissRoms" revision="o8.0" />
  <project path="system/sepolicy" name="platform_system_sepolicy" remote="BlissRoms" revision="o8.0" />
  <project path="packages/providers/MediaProvider" name="platform_packages_providers_MediaProvider" remote="BlissRoms" revision="o8.0" />

  <!--                       -->

</manifest>
